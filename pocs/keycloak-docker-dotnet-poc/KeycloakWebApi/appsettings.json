{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "AuthorizationUrl" : "http://localhost:8080/realms/myrealm/protocol/openid-connect/auth"
  },
  "Authentication": {
    "Audience": "account",
    "MetadataAddress": "http://keycloak:8080/realms/myrealm/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/realms/myrealm"

  }
}
